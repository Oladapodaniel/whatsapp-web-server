{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/socket.io/**",
      "destination": "/socket.io"
    },
    {
      "source": "/socket.io",
      "destination": "/_socket.io"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}